{
  "name": "remark-github-plugin",
  "version": "0.1.0",
  "description": "Remark plugin to inline GitHub source code into markdown file as code blocks",
  "keywords": [
    "remark",
    "github",
    "unified"
  ],
  "author": "Huy Nguyen <me@huynguyen.io> (https://www.huynguyen.io/)",
  "license": "MIT",
  "dependencies": {
    "github-url-parse": "0.1.0"
  },
  "main": "lib/index.js",
  "scripts": {
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "commit": "commit",
    "tslint": "tslint --project tsconfig.json --config tslint.json",
    "tslint:fix": "tslint --project tsconfig.json --config tslint.json --fix",
    "test": "jest --verbose --coverage",
    "test:watch": "jest --watch --verbose",
    "eslint": "eslint .",
    "eslint:fix": "eslint . --fix"
  },
  "devDependencies": {
    "@babel/core": "7.0.0-beta.42",
    "@babel/preset-env": "7.0.0-beta.42",
    "@cid-harvard/eslint-config": "0.1.2",
    "@cid-harvard/tslint-config": "0.3.0",
    "@commitlint/cli": "6.1.3",
    "@commitlint/config-conventional": "6.1.3",
    "@commitlint/prompt-cli": "6.1.3",
    "@types/node-fetch": "1.6.8",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "8.2.2",
    "babel-jest": "22.4.3",
    "dotenv": "5.0.1",
    "eslint": "4.19.1",
    "eslint-plugin-jest": "21.15.0",
    "eslint-plugin-react": "7.7.0",
    "husky": "0.14.3",
    "jest": "22.4.3",
    "ts-jest": "22.4.2",
    "tslint": "5.8.0",
    "typescript": "2.8.1"
  }
}
